GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ObjectAL-for-iPhone" "${PODS_ROOT}/Headers/Public/PureLayout"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ObjectAL-for-iPhone" -isystem "${PODS_ROOT}/Headers/Public/PureLayout"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ObjectAL-for-iPhone" -l"PureLayout" -framework "AVFoundation" -framework "AudioToolbox" -framework "OpenAL"
PODS_ROOT = ${SRCROOT}/Pods